{
    "compileOnSave": false,
    "compilerOptions": {
        "module": "ES6",
        "moduleResolution": "node",
        "target": "ES5",
        "sourceMap": true,
        "jsx": "react",
        "jsxFactory": "Snabbdom.createElement",
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "strict": true,
        "esModuleInterop": true,
        "strictFunctionTypes": false,
        "allowJs": true,
        "watch": true,
        "baseUrl": "./",
        "outDir": "./build",
        "types": [
            "webpack-env",
            "ramda"
        ],
        "lib": [
            "es2017",
            "es6",
            "es5",
            "dom"
        ]
    },
    "exclude": [
        "node_modules",
        "build",
        "src/src.2"
    ]
}