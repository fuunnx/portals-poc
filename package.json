      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 1 version"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "include": [
      "src/components"
    ],
    "reporter": [
      "html",
      "text-summary"
