  "scripts": {
    "format": "prettier --write '{src,test}/**/*.{js,jsx,ts,tsx}'",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack.config.js",
    "test": "jest --watch --notify --runInBand",
    "postinstall": "patch-package",
    "prettier": "prettier --config .prettierrc --write 'src/**/*{.ts,.tsx,.css,.scss}'",
